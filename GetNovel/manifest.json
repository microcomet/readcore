{
    "name":"get novel",
    "version":"1.0",
    "description":"extension has real use that i made",
    "background":{"scripts":["js/org/jquery-2.1.4.js",
                             "js/module/background.js"]},
    "page_action":{
       	"default_icon":"images/icon16.png",
       	"default_title":"Get Novel"
    },
    "content_scripts":[
      {
        "matches":["http://www.biquge.la/*"],
        "js":["js/org/jquery-2.1.4.js",
              "js/module/getNovel.js"],
        "css":["css/getNovel.css"]
      }
    ],
    "icons":{"32":"images/icon32.png",
             "48":"images/icon48.png",
             "64":"images/icon64.png"},
    "manifest_version":2,
    "permissions":["activeTab",
                   "tabs"]
}